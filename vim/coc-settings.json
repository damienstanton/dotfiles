	"diagnostic.virtualText": true,
	"languageserver": {
		"go": {
		  "command": "gopls",
		  "rootPatterns": ["go.mod"],
		  "trace.server": "verbose",
		  "filetypes": ["go"]
		},
		 "haskell": {
			 "command": "haskell-language-server-wrapper",
